<section id="cd-ocha-services" class="cd-ocha-services cd-footer__section cd-footer__section--ocha-services" aria-label="{{ 'OCHA Services'|t }}">
  <h2 id="cd-ocha-services-title">{{ 'OCHA Services and related platforms'|t }}</h2>

  <div class="cd-ocha-services__content">
    <div class="cd-ocha-services__inner">
      <div class="cd-ocha-services__section">
        <ul class="cd-ocha-services__list">
          {% block related_platforms %}
          <li class="cd-ocha-services__link"><a href="https://cerf.un.org/">{{ 'Central Emergency Response Fund'|t }}</a></li>
          <li class="cd-ocha-services__link"><a href="https://fts.unocha.org/">{{ 'Financial Tracking Service'|t }}</a></li>
          <li class="cd-ocha-services__link"><a href="https://gdacs.org/">{{ 'Global Disaster Alert & Coordination System'|t }}</a></li>
          {% endblock %}
        </ul>
      </div>
      <div class="cd-ocha-services__section">
        <ul class="cd-ocha-services__list">
          {% block other_services_first %}
          <li class="cd-ocha-services__link"><a href="https://data.humdata.org/">{{ 'Humanitarian Data Exchange'|t }}</a></li>
          <li class="cd-ocha-services__link"><a href="https://humanitarianaction.info/">{{ 'Humanitarian Action'|t }}</a></li>
          <li class="cd-ocha-services__link"><a href="https://www.insarag.org/">{{ 'International Search and Rescue Advisory'|t }}</a></li>
          {% endblock %}
        </ul>
      </div>
      <div class="cd-ocha-services__section">
        <ul class="cd-ocha-services__list">
          {% block other_services_second %}
          <li class="cd-ocha-services__link"><a href="https://interagencystandingcommittee.org">{{ 'Inter-Agency Standing Committee'|t }}</a></li>
          <li class="cd-ocha-services__link"><a href="https://pfdata.unocha.org/">{{ 'Pooled Funds Data Hub'|t }}</a></li>
          <li class="cd-ocha-services__link"><a href="https://reliefweb.int/">{{ 'ReliefWeb'|t }}</a></li>
          {% endblock %}
        </ul>
      </div>
      <div class="cd-ocha-services__section">
        <ul class="cd-ocha-services__list">
          <li class="cd-ocha-services__link"><a href="https://response.reliefweb.int/">{{ 'ReliefWeb Response'|t }}</a></li>
          <li class="cd-ocha-services__link">
            <a class="cd-ocha-services__see-all" href="https://www.unocha.org/ocha-digital-services" target="_blank" rel="noopener">
              {{ 'More OCHA Services'|t }}
              <span class="visually-hidden">Opens in a new window</span>
              <svg class="cd-icon cd-icon--external-link" width="16" height="16" aria-hidden="true" focusable="false">
                <use xlink:href="#cd-icon--external-link"></use>
              </svg>
            </a>
          </li>
        </ul>
      </div>
    </div>


